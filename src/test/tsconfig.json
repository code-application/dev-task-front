{
  "extends": "../../tsconfig.json",
  "include": ["./**/*", "../**/*"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "bundler",
    "baseUrl": "../../",
    "plugins": [
      {
        "name": "next",
        "@/*": ["./src/*"]
      }
    ],
    "paths": {
      "@/components/*": ["components/*"],
      "@/libs/*": ["libs/*"],
      "@/*": ["./src/*"]
    }
  }
}
